{
    "name": "brainflip",
    "displayName": "Brainflip",
    "description": "Support for the Brainflip teaching language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "brainflip",
            "aliases": ["Brainflip", "brainflip"],
